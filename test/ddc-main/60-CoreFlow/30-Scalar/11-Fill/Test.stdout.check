-- | Single map over input series, writing into existing vector.
module Test with
letrec {
  test : [p : Proc].[k : Rate].Vector# Int# -> Series# p k k Int# -> Unit
    = /\(p : Proc)./\(k : Rate).
       \(v : Vector# Int#).\(s : Series# p k k Int#).
      let v$buf = vbuf# [Int#] v in
      let _
            = loop# [k]
                  (\(x0 : Nat#).
                   let s$elem = next# [Int#] [p] [k] [k] s x0 in
                   let x1 = mul# [Int#] s$elem 2i# in
                   let x3$elem = add# [Int#] x1 1i# in
                   let _ = vwrite# [Int#] v$buf x0 x3$elem in
                   ()) in
      ()
}
