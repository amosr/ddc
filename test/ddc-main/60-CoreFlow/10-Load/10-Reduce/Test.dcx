:set lang Flow
:set Synth


-- Single series being eaten by two consumers.
:load..
module Test with letrec
test    [p : Proc] [k : Rate]
        (u1 u2 : Ref# Int#)
        (elems : Series# p k k Int#) : Process# p k
 =      pjoin#  (sreduce# u1 add# 0i# elems)
                (sreduce# u2 mul# 1i# elems)
;;
